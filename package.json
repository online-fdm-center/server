{
  "name": "online.fdm.center",
  "version": "1.0.0",
  "description": "server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "migrate": "node ./dist/src/migrate",
    "prestart": "npm run build",
    "start": "HOST=0.0.0.0 node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.1.1",
    "@loopback/boot": "^1.2.0",
    "@loopback/context": "^1.10.0",
    "@loopback/core": "^1.4.0",
    "@loopback/openapi-v3": "^1.3.4",
    "@loopback/repository": "^1.4.0",
    "@loopback/rest": "^1.10.1",
    "@loopback/rest-explorer": "^1.1.15",
    "@loopback/service-proxy": "^1.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/multer": "^1.3.7",
    "@types/passport-local": "^1.0.33",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^3.0.4",
    "dotenv": "^6.2.0",
    "loopback-connector-mysql": "^5.3.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-local": "^1.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.4.2",
    "@loopback/testlab": "^1.2.3",
    "@loopback/tslint-config": "^2.0.4",
    "@types/node": "^10.12.21",
    "@types/passport": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "tslint": "^5.12.0",
    "typescript": "^3.4.3"
  }
}
